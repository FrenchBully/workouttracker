<h1>Sign Up!</h1>

<%= form_for @user do |f| %>
     <%= f.label :first_name %>
     <%= f.text_field :firstname %>

     <%= f.label :last_name %>
     <%= f.text_field :lastname %>

     <%= f.label :email %>
     <%= f.text_field :email %>

     <%= f.label :password %>
     <%= f.text_field :password %>

     <%= f.submit "Sign Me Up!" %>
<% end %>